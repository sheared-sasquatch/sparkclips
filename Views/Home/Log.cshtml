@model IEnumerable<sparkclips.Models.LogEntry>
@{
    ViewData["Title"] = "My haircut log";
}
@section Styles {
    <link rel="stylesheet" href="~/css/log.css" />
}
<h2>@ViewData["Title"].</h2>

<div class="row">


    <div class="col-md-4">

        <div class="list-group">
        @foreach (var item in Model)
        {
            <a asp-action="LogDetail" asp-route-id="@item.ID" class="list-group-item">@Html.DisplayFor(modelItem => item.BarberShop)</a>
        }
        </div>

    </div>
    <div class="col-md-8 hidden-xs">
        @Html.Partial("LogDetailStatic.cshtml")
    </div>

</div>